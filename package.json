{
  "name": "imhere-qgen",
  "version": "1.0.15",
  "description": "An SVG QR Code Generator I made for a project.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
