  },
  "homepage": "https://github.com/naquino14/qgen#readme",
  "devDependencies": {
    "@types/react": "^18.0.21",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
